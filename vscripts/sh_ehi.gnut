#if CLIENT || UI 
array<EHI> function GetPlayerEHIArrayOfTeam( int team )
{
	#if(false)

#elseif CLIENT || UI 
		array<EHI> out = []
		foreach ( EHI ehi, EHIScriptStruct ehiss in fileLevel.eehScriptStructMap )
		{
			if ( !ehiss.isPlayer )
				continue

			if ( ehiss.team != team )
				continue

			out.append( ehi )
		}
		return out
	#endif
}
#endif


